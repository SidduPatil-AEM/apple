<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/apple/settings/wcm/templates/content-page-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="apple/components/structure/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="apple/components/content/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/apple/us/en/site/header/master"/>
            <responsivegrid
                editable="{Boolean}true"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <breadcrumb
                    cq:tags="[we-retail:activity/biking]"
                    jcr:created="{Date}2020-04-21T03:37:43.159-05:00"
                    jcr:createdBy="admin"
                    jcr:lastModified="{Date}2020-04-21T03:49:07.465-05:00"
                    jcr:lastModifiedBy="admin"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="apple/components/content/breadcrumb"
                    hideCurrent="false"
                    showHidden="false"
                    startLevel="1"/>
            </responsivegrid>
            <experiencefragment-footer
                jcr:primaryType="nt:unstructured"
                sling:resourceType="apple/components/content/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/apple/us/en/site/footer/master"/>
        </root>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
